{
  "name": "hypnos",
  "version": "0.0.2",
  "description": "An app to test GraphQL calls on RESTful APIs, via Apollo.",
  "main": "electron.js",
  "homepage": "./",
  "scripts": {
    "start-electron": "electron .",
    "start-electron-prod": "ELECTRON_IS_DEV=0 npm run start-electron",
    "electron-build": "electron-builder -mwl",
    "clean-build": "rm -rf build/ .cache dist/",
    "react-start": "NODE_ENV=development parcel -p 3000 index.html --out-dir build",
    "react-build": "NODE_ENV=production parcel build index.html --out-dir build --public-url ./",
    "start": "npm run react-start & wait-on http://localhost:3000 && ELECTRON_IS_DEV=1 npm run start-electron",
    "build": "npm run clean-build && npm run react-build && ELECTRON_IS_DEV=0 npm run electron-build",
    "test": "jest --verbose"
  },
  "keywords": [
    "Apollo",
    "Apollo-Link-Rest",
    "Electron",
    "Jest",
    "Enzyme",
    "Puppeteer",
    "Testing",
    "Dexie",
    "IndexDB",
    "React",
    "Parcel",
    "GraphQL",
    "REST API",
    "API",
    "Query",
    "Mutation"
  ],
  "author": {
    "name": "Sophie Nye",
    "email": "sophie.nye@gmail.com"
  },
  "contributors": [
    "Dillon Garrett <dillon.garrett.dev@gmail.com",
    "Will Robinson <wrobinson91@gmail.com"
  ],
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-rest": "^0.7.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "codemirror": "^5.48.2",
    "context": "0.0.1",
    "css-loader": "^3.1.0",
    "dexie": "^2.0.4",
    "dotenv": "^8.0.0",
    "electron-is-dev": "^1.1.0",
    "fake-indexeddb": "^2.1.1",
    "graphql": "^14.4.2",
    "graphql-anywhere": "^4.2.4",
    "graphql-tag": "^2.10.1",
    "react": "^16.9.0",
    "react-apollo": "^2.5.8",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.8.6",
    "react-modal": "^3.9.1",
    "react-tabs": "^3.0.0",
    "react-test-renderer": "^16.9.0",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "electron": "^5.0.7",
    "electron-builder": "^21.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-packager": "^14.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "puppeteer": "^1.19.0",
    "regenerator-runtime": "^0.13.3",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "com.hypnos.app",
    "productName": "Hypnos",
    "files": [
      "./electron.js",
      "./build/**/*",
      "./assets/hypnos-icon.png"
    ],
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg",
      "icon": "./assets/hypnos-icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "./assets/hypnos-icon.png"
    },
    "linux": {
      "target": "deb",
      "icon": "./assets/hypnos-icon.png",
      "category": "Development"
    }
  }
}
